#include "holberton.h"

/**
* times_table - integer table
* description: integer table
*
* Return: 0
*/

void times_table(void)
{
	int a, b, c;

	for (a = 0; a <= 9; a++)
{
	for (b = 0 ; b <= 9 ; b++)
{
	c = a * b;

/* this is: where i stopped */	
	if (c != 0)

	_putchar('0' + 1 );
	_putchar('0' + );

	if (b <= 9)
{
	_putchar(',');
	_putchar(' ');

	if (c <= 9)
	_putchar (' ');
}
else
{
	_putchar('\n');
}
}
}
}
